@{
    ViewData["Title"] = "General Motors Co. Omega CD 4.1 1995";
}
<h2>@ViewData["Title"]</h2>

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/omega-cd-41-1998.svg" alt="ASP.NET" class="img-responsive" />
            <div class="carousel-caption" role="option">
            </div>
        </div>
        <div class="item">
            <img src="~/images/omega-cd-4-1-modelo-1998-da-chevrolet.svg" alt="Visual Studio" class="img-responsive" />
            <div class="carousel-caption" role="option">
            </div>
        </div>
        <div class="item">
            <img src="~/images/motor-do-omega-cd-4-1-modelo-1998-da-chevrolet.svg" alt="Microsoft Azure" class="img-responsive" />
            <div class="carousel-caption" role="option">
            </div>
        </div>
        <div class="item">
            <img src="~/images/interior-do-omega-cd-4-1-modelo-1998-da-chevrolet.svg" alt="Microsoft Azure" class="img-responsive" />
            <div class="carousel-caption" role="option">
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<h2>Car Specs</h2>
<div class="row body-content">
    <div class="col-md-3">
        <h2>Engine:</h2>
        <h2>Engine Type:</h2>
        <h2>Transmission:</h2>
        <h2>Weight:</h2>
        <h2>Injection:</h2>
        <h2>Horsepower:</h2>
    </div>
    <div class="col-md-3">
        <h2>C41GE 4.1 Litre</h2>
        <h2>6 cylinder inline</h2>
        <h2>MT 5 Gears/RWD</h2>
        <h2>1530 kg</h2>
        <h2>Multipoint Injection</h2>
        <h2>168 HP</h2>
    </div>
    <div class="col-md-3">
        <h1>Price:</h1>
    </div>
    <div class="col-md-3">
        <h2>$13000</h2>
        <li><a class="btn btn-success" href="http://localhost:62862/Home/Thanks_for_your_Purchase/">Buy This Car</a></li>
        <li><a class="btn btn-warning" href="http://localhost:62862/Home/General_Motors_cars">View other models of this brand</a></li>
        <li><a class="btn btn-danger" href="http://localhost:62862/Home">Back to Start</a></li>
    </div>
</div>
